        <!-- general form elements -->
        <div class="card card-dark">
            <div class="card-header">
                <h3 class="card-title">Editer</h3>
            </div>
            <form asp-action="Edit">
                <partial name="BasesEntity/_PersonBase" />
                <partial name="BasesEntity/_BaseEntity" />

                <!--Submit btn -->
                <div class="card-footer text-center">
                    <button type="submit" value="Create" class="btn btn-success" title="Enregistrer"><i class="fas fa-save"></i></button>
                </div>

            </form>







        </div>

    </div>
</div>


